{
  "name": "lnrpc",
  "version": "0.4.0",
  "description": "typed node.js gRPC client for lightningnetwork/lnd",
  "main": "index.js",
  "types": "types/index.d.ts",
  "config": {
    "lnd-release-tag": "v0.5.1-beta",
    "protoc-version": "3.5.1"
  },
  "scripts": {
    "test": "./node_modules/.bin/eslint . && ./node_modules/.bin/mocha ./test/**/*.test.js",
    "start": "node ./index.js",
    "lint": "tslint -c tslint.json '{lib,test,types}/**/*.ts'",
    "generate": "sh ./generate_types.sh ${npm_package_config_lnd_release_tag} ${npm_package_config_protoc_version}",
    "postinstall": "napa lightningnetwork/lnd:lnd#${npm_package_config_lnd_release_tag}"
  },
  "repository": "https://github.com/RadarRelay/lnrpc",
  "keywords": [
    "lnd",
    "lightning network",
    "grpc",
    "client",
    "bitcoin"
  ],
  "author": "Radar",
  "license": "MIT",
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "@types/google-protobuf": "^3.2.7",
    "grpc": "^1.14.0-pre2",
    "napa": "^3.0.0",
    "pkg-dir": "^2.0.0",
    "ts-protoc-gen": "^0.8.0"
  },
  "devDependencies": {
    "eslint": "^4.7.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.2.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">= 8.*"
  },
  "napa-config": {
    "cache": false,
    "log-level": "error"
  }
}
