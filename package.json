{
  "name": "lnrpc",
  "version": "0.3.0",
  "description": "User-centric Node.js gRPC client for lightningnetwork/lnd",
  "main": "index.js",
  "scripts": {
    "test":
      "./node_modules/.bin/eslint . && ./node_modules/.bin/mocha ./test/**/*.test.js",
    "start": "node ./index.js",
    "postinstall": "napa lightningnetwork/lnd:lnd"
  },
  "repository": "https://github.com/Matt-Jensen/lnrpc",
  "keywords": ["lnd", "lightning network", "grpc", "client", "bitcoin", "litecoin"],
  "author": "Matt-Jensen",
  "license": "MIT",
  "dependencies": {
    "grpc": "^1.6.0",
    "napa": "^3.0.0",
    "pkg-dir": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^4.7.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^3.5.3"
  },
  "engines": {
    "node": ">= 8.*"
  },
  "napa-config": {
    "cache": false,
    "log-level": "error"
  }
}
